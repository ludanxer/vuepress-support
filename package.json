{
  "name": "vuepress-support",
  "version": "1.0.1",
  "scripts": {
    "docs:dev": "vuepress dev docs --temp .temp",
    "docs:build": "vuepress build docs",
    "build": "docs:build && mv docs/.vuepress/dist public",
    "docs:build-pages": "VUEPRESS_BASE=/vuepress-support/ vuepress build docs"
  },
  "directories": {
    "doc": "docs"
  },
